{
  "version": 2,
  "name": "app",
  "scope": "trainit",
  "builds": [
      { "src": "index.html", "use": "@now/static" },
      { "src": "/api/src/php/main/*.php", "use": "@now/php" }
  ],
  "routes": [
      { "src": "/api/(.*)", "dest": "/api/index.html" },
      { "src": "/api/(.*)", "dest": "/api/src/php/main/validation.php" }
  ]
}